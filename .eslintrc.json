{
  "root": true,
  "env": {
    "es2017": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars": [
        "warn", {
            "varsIgnorePattern": "^_",
            "args": "none"
        }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "settings": {
    "react": {
      "version": "16.6.3"
    }
  }
}
